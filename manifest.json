{
  "version": "1.0",
  "manifest_version": 2,
  "name": "Random Food",
  "icons": {"128": "photos/food.png"},
  "permissions": [
    "geolocation",
    "*://*.yelp.com/*"
  ],
  "browser_action": {
    "default_icon": "photos/food.png",
    "default_popup": "popup.html",
    "default_title": "Foody"
  },
  "content_security_policy": "script-src 'self' https://unpkg.com/axios/dist/axios.min.js; object-src 'self'"
}
